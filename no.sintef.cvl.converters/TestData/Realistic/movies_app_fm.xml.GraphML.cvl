<?xml version="1.0" encoding="ASCII"?>
<cvl:ConfigurableUnit xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cvl="http:///cvl.ecore" name="Movies_App">
  <ownedVSpec xsi:type="cvl:Choice" name="Movies_App" isImpliedByParent="true">
    <child xsi:type="cvl:Choice" name="GUI" isImpliedByParent="true">
      <groupMultiplicity upper="1" lower="1"/>
      <child xsi:type="cvl:Choice" name="Thin"/>
      <child xsi:type="cvl:Choice" name="Rich"/>
    </child>
    <child xsi:type="cvl:Choice" name="DB" isImpliedByParent="true">
      <groupMultiplicity upper="1" lower="1"/>
      <child xsi:type="cvl:Choice" name="Local"/>
      <child xsi:type="cvl:Choice" name="Remote">
        <child xsi:type="cvl:Choice" name="Cache_Policy" isImpliedByParent="true"/>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="Network" isImpliedByParent="true">
      <groupMultiplicity upper="1" lower="1"/>
      <child xsi:type="cvl:Choice" name="Wifi"/>
      <child xsi:type="cvl:Choice" name="Bluetooth"/>
    </child>
    <child xsi:type="cvl:Choice" name="GPS">
      <child xsi:type="cvl:Choice" name="Built_In"/>
    </child>
  </ownedVSpec>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="Movies_App"/>
</cvl:ConfigurableUnit>

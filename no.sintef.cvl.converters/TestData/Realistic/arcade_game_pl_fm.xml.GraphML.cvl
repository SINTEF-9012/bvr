<?xml version="1.0" encoding="ASCII"?>
<cvl:ConfigurableUnit xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cvl="http:///cvl.ecore" name="ArcadeGame">
  <ownedVSpec xsi:type="cvl:Choice" name="ArcadeGame" isImpliedByParent="true">
    <child xsi:type="cvl:Choice" name="UseCases" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="CheckPreviousBestScore"/>
      <child xsi:type="cvl:Choice" name="SaveScore"/>
      <child xsi:type="cvl:Choice" name="SaveGame" isImpliedByParent="true"/>
      <child xsi:type="cvl:Choice" name="ExitGame" isImpliedByParent="true"/>
      <child xsi:type="cvl:Choice" name="InstallGame" isImpliedByParent="true"/>
      <child xsi:type="cvl:Choice" name="UninstallGame" isImpliedByParent="true"/>
      <child xsi:type="cvl:Choice" name="ListGame" isImpliedByParent="true"/>
      <child xsi:type="cvl:Choice" name="PlayGame" isImpliedByParent="true">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="PlayBrickles"/>
        <child xsi:type="cvl:Choice" name="PlayPong"/>
        <child xsi:type="cvl:Choice" name="PlayBowling"/>
        <child xsi:type="cvl:Choice" name="Initialization">
          <child xsi:type="cvl:Choice" name="AnimationLoop" isImpliedByParent="true"/>
        </child>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="ClassDiagram" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="GameSprite" isImpliedByParent="true">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="SpritePair"/>
        <child xsi:type="cvl:Choice" name="Rectangle">
          <child xsi:type="cvl:Choice" name="Size" isImpliedByParent="true"/>
          <child xsi:type="cvl:Choice" name="Point" isImpliedByParent="true"/>
        </child>
        <child xsi:type="cvl:Choice" name="MovableSprite">
          <groupMultiplicity upper="-1" lower="1"/>
          <child xsi:type="cvl:Choice" name="Velocity"/>
          <child xsi:type="cvl:Choice" name="Paddle">
            <groupMultiplicity upper="-1" lower="1"/>
            <child xsi:type="cvl:Choice" name="TopPaddle"/>
            <child xsi:type="cvl:Choice" name="BottomPaddle"/>
          </child>
          <child xsi:type="cvl:Choice" name="Puck"/>
          <child xsi:type="cvl:Choice" name="BowlingBall"/>
          <child xsi:type="cvl:Choice" name="BowlingPin"/>
        </child>
        <child xsi:type="cvl:Choice" name="StationarySprite">
          <groupMultiplicity upper="-1" lower="1"/>
          <child xsi:type="cvl:Choice" name="Wall">
            <groupMultiplicity upper="-1" lower="1"/>
            <child xsi:type="cvl:Choice" name="Leftpong"/>
            <child xsi:type="cvl:Choice" name="Rightpont"/>
            <child xsi:type="cvl:Choice" name="Leftbrickles"/>
            <child xsi:type="cvl:Choice" name="Rightbrickles"/>
          </child>
          <child xsi:type="cvl:Choice" name="Brick"/>
          <child xsi:type="cvl:Choice" name="BrickPile"/>
          <child xsi:type="cvl:Choice" name="Ceilingbrickles"/>
          <child xsi:type="cvl:Choice" name="Floorbrickles"/>
          <child xsi:type="cvl:Choice" name="Lane"/>
          <child xsi:type="cvl:Choice" name="Gutter"/>
          <child xsi:type="cvl:Choice" name="Edge"/>
          <child xsi:type="cvl:Choice" name="EndofAlley"/>
          <child xsi:type="cvl:Choice" name="RackofPins"/>
          <child xsi:type="cvl:Choice" name="ScoreBoard"/>
          <child xsi:type="cvl:Choice" name="Floorpong"/>
          <child xsi:type="cvl:Choice" name="Ceilingpong"/>
          <child xsi:type="cvl:Choice" name="DividingLine"/>
        </child>
      </child>
      <child xsi:type="cvl:Choice" name="Pucksupply"/>
      <child xsi:type="cvl:Choice" name="Board" isImpliedByParent="true">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="PongBoard"/>
        <child xsi:type="cvl:Choice" name="BricklesBoard"/>
        <child xsi:type="cvl:Choice" name="BowlingBoard"/>
      </child>
      <child xsi:type="cvl:Choice" name="Menu" isImpliedByParent="true">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="Pong"/>
        <child xsi:type="cvl:Choice" name="Brickles"/>
        <child xsi:type="cvl:Choice" name="Bowling"/>
      </child>
      <child xsi:type="cvl:Choice" name="GameMenu" isImpliedByParent="true">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="PongGameMenu"/>
        <child xsi:type="cvl:Choice" name="BricklesGameMenu"/>
        <child xsi:type="cvl:Choice" name="BowlingGameMenu"/>
      </child>
    </child>
  </ownedVSpec>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not PlayBrickles or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="PlayBrickles or not Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not PlayPong or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="PlayPong or not Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not PlayBowling or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="PlayBowling or not Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not TopPaddle or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not BottomPaddle or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Leftpong or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Rightpont or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not ScoreBoard or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Floorpong or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Ceilingpong or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not DividingLine or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not PongBoard or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not PongGameMenu or Pong"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not BowlingBoard or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not BowlingBall or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not BowlingPin or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Lane or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Gutter or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Edge or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not EndofAlley or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not RackofPins or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not BowlingGameMenu or Bowling"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not BricklesGameMenu or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not BricklesBoard or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Leftbrickles or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Rightbrickles or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not BrickPile or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Brick or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Ceilingbrickles or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Floorbrickles or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not Pucksupply or Brickles"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="ArcadeGame"/>
</cvl:ConfigurableUnit>

<?xml version="1.0" encoding="ASCII"?>
<cvl:ConfigurableUnit xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cvl="http:///cvl.ecore" name="RVM">
  <ownedVSpec xsi:type="cvl:Choice" name="RVM" isImpliedByParent="true">
    <child xsi:type="cvl:Choice" name="CrateUnit">
      <groupMultiplicity upper="1" lower="1"/>
      <child xsi:type="cvl:Choice" name="CRS2">
        <child xsi:type="cvl:Choice" name="CRS2_ColorDetector"/>
        <child xsi:type="cvl:Choice" name="CRS2_CrateScale"/>
      </child>
      <child xsi:type="cvl:Choice" name="CRS3">
        <child xsi:type="cvl:Choice" name="CRS3_ColorDetector"/>
        <child xsi:type="cvl:Choice" name="CRS3_CrateScale"/>
      </child>
      <child xsi:type="cvl:Choice" name="CamCrate">
        <child xsi:type="cvl:Choice" name="ColorCamera"/>
        <child xsi:type="cvl:Choice" name="LogoCamera"/>
        <child xsi:type="cvl:Choice" name="CamCrate_CrateScale"/>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="BottleUnit" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="Display" isImpliedByParent="true">
        <groupMultiplicity upper="1" lower="1"/>
        <child xsi:type="cvl:Choice" name="Display2line"/>
        <child xsi:type="cvl:Choice" name="DisplayBW"/>
        <child xsi:type="cvl:Choice" name="DisplayColor"/>
        <child xsi:type="cvl:Choice" name="DisplayTouch"/>
      </child>
      <child xsi:type="cvl:Choice" name="Scale"/>
      <child xsi:type="cvl:Choice" name="Metal"/>
      <child xsi:type="cvl:Choice" name="Barcode"/>
      <child xsi:type="cvl:Choice" name="BMS"/>
      <child xsi:type="cvl:Choice" name="SecurityMarkReader">
        <groupMultiplicity upper="1" lower="1"/>
        <child xsi:type="cvl:Choice" name="SMR1"/>
        <child xsi:type="cvl:Choice" name="SMR2"/>
      </child>
      <child xsi:type="cvl:Choice" name="Printer" isImpliedByParent="true">
        <child xsi:type="cvl:Choice" name="Printer1" isImpliedByParent="true"/>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="Product_group" isImpliedByParent="true">
      <groupMultiplicity upper="1" lower="1"/>
      <child xsi:type="cvl:Choice" name="FrontEnd">
        <child xsi:type="cvl:Choice" name="Machine_type_frontend" isImpliedByParent="true">
          <groupMultiplicity upper="1" lower="1"/>
          <child xsi:type="cvl:Choice" name="RVM1"/>
          <child xsi:type="cvl:Choice" name="RVM2"/>
          <child xsi:type="cvl:Choice" name="RVM3"/>
          <child xsi:type="cvl:Choice" name="RVM4"/>
          <child xsi:type="cvl:Choice" name="RVM5"/>
        </child>
        <child xsi:type="cvl:Choice" name="Backroom" isImpliedByParent="true">
          <child xsi:type="cvl:Choice" name="SortingIntersection"/>
          <child xsi:type="cvl:Choice" name="Backroom_details" isImpliedByParent="true">
            <groupMultiplicity upper="1" lower="1"/>
            <child xsi:type="cvl:Choice" name="MasterPack">
              <groupMultiplicity upper="1" lower="1"/>
              <child xsi:type="cvl:Choice" name="LeftTurn"/>
              <child xsi:type="cvl:Choice" name="LeftStraight"/>
              <child xsi:type="cvl:Choice" name="RightTurn"/>
              <child xsi:type="cvl:Choice" name="RightStraight"/>
            </child>
            <child xsi:type="cvl:Choice" name="SoftDrop"/>
            <child xsi:type="cvl:Choice" name="Logistima">
              <child xsi:type="cvl:Choice" name="HSG">
                <child xsi:type="cvl:Choice" name="HSG_ekstra"/>
              </child>
              <child xsi:type="cvl:Choice" name="SCC_L">
                <child xsi:type="cvl:Choice" name="SCC_L_ekstra"/>
              </child>
              <child xsi:type="cvl:Choice" name="SCC_R">
                <child xsi:type="cvl:Choice" name="SCC_R_ekstra"/>
              </child>
              <child xsi:type="cvl:Choice" name="Termination" isImpliedByParent="true">
                <groupMultiplicity upper="1" lower="1"/>
                <child xsi:type="cvl:Choice" name="EBR_L"/>
                <child xsi:type="cvl:Choice" name="EBR_R"/>
                <child xsi:type="cvl:Choice" name="Logistima_SoftDrop"/>
              </child>
            </child>
            <child xsi:type="cvl:Choice" name="RaiserBord"/>
          </child>
        </child>
      </child>
      <child xsi:type="cvl:Choice" name="InPack">
        <child xsi:type="cvl:Choice" name="Machine_type_inpack" isImpliedByParent="true">
          <groupMultiplicity upper="1" lower="1"/>
          <child xsi:type="cvl:Choice" name="MTI1"/>
          <child xsi:type="cvl:Choice" name="MTI2"/>
          <child xsi:type="cvl:Choice" name="MTI3"/>
          <child xsi:type="cvl:Choice" name="MTI4"/>
          <child xsi:type="cvl:Choice" name="MTI5">
            <child xsi:type="cvl:Choice" name="MTI5_SoftDrop" isImpliedByParent="true"/>
          </child>
          <child xsi:type="cvl:Choice" name="MTI6"/>
        </child>
        <child xsi:type="cvl:Choice" name="LowerCabinet" isImpliedByParent="true">
          <groupMultiplicity upper="1" lower="1"/>
          <child xsi:type="cvl:Choice" name="Single"/>
          <child xsi:type="cvl:Choice" name="Dual"/>
        </child>
      </child>
    </child>
  </ownedVSpec>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="SecurityMarkReader implies Barcode"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="ColorCamera iff LogoCamera"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="RVM1 implies DisplayBW"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="RVM1 and CrateUnit implies CRS2"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="RVM2 implies DisplayColor"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="RVM2 and CrateUnit implies CRS2"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="RVM3 implies DisplayColor or DisplayTouch"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="RVM3 and CrateUnit implies CamCrate"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="RVM4 implies Display2line"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="RVM4 and CrateUnit implies CRS3"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="MTI1 implies DisplayBW"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="MTI2 implies DisplayColor"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="MTI3 implies DisplayBW"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="MTI5 implies DisplayColor"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="MTI6 implies DisplayColor"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="MTI6 implies not SoftDrop and Single"/>
</cvl:ConfigurableUnit>

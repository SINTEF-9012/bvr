<?xml version="1.0" encoding="ASCII"?>
<cvl:ConfigurableUnit xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cvl="http:///cvl.ecore" name="search_engine_PL">
  <ownedVSpec xsi:type="cvl:Choice" name="search_engine_PL" isImpliedByParent="true">
    <child xsi:type="cvl:Choice" name="page_translation"/>
    <child xsi:type="cvl:Choice" name="doc_type" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="html" isImpliedByParent="true"/>
      <child xsi:type="cvl:Choice" name="image">
        <groupMultiplicity upper="1" lower="1"/>
        <child xsi:type="cvl:Choice" name="jpg"/>
        <child xsi:type="cvl:Choice" name="gif"/>
        <child xsi:type="cvl:Choice" name="svg"/>
      </child>
      <child xsi:type="cvl:Choice" name="video"/>
    </child>
    <child xsi:type="cvl:Choice" name="search_by_language">
      <groupMultiplicity upper="-1" lower="1"/>
      <child xsi:type="cvl:Choice" name="portuguese"/>
      <child xsi:type="cvl:Choice" name="english"/>
      <child xsi:type="cvl:Choice" name="spanish"/>
    </child>
    <child xsi:type="cvl:Choice" name="page_preview"/>
  </ownedVSpec>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not search_by_language or page_translation"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="not page_preview or not svg"/>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="search_engine_PL"/>
</cvl:ConfigurableUnit>

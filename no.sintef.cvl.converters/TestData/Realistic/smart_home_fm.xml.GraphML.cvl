<?xml version="1.0" encoding="ASCII"?>
<cvl:ConfigurableUnit xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cvl="http:///cvl.ecore" name="SmartHome">
  <ownedVSpec xsi:type="cvl:Choice" name="SmartHome" isImpliedByParent="true">
    <child xsi:type="cvl:Choice" name="EnvironmentControl" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="MeasurementUnits" isImpliedByParent="true">
        <child xsi:type="cvl:Choice" name="Celsius"/>
        <child xsi:type="cvl:Choice" name="Fahrenheit"/>
      </child>
      <child xsi:type="cvl:Choice" name="TemperatureManagement" isImpliedByParent="true">
        <child xsi:type="cvl:Choice" name="HeatingManagement" isImpliedByParent="true"/>
        <child xsi:type="cvl:Choice" name="SmartHeatingManagement"/>
      </child>
      <child xsi:type="cvl:Choice" name="WindowsManagement" isImpliedByParent="true">
        <child xsi:type="cvl:Choice" name="ManualWindows" isImpliedByParent="true"/>
        <child xsi:type="cvl:Choice" name="AutomaticWindows"/>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="LightManagement" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="SmartLightManagement"/>
      <child xsi:type="cvl:Choice" name="Pre_definedValues"/>
      <child xsi:type="cvl:Choice" name="SimpleControl" isImpliedByParent="true"/>
    </child>
    <child xsi:type="cvl:Choice" name="GUI" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="TouchScreen"/>
      <child xsi:type="cvl:Choice" name="Mobile"/>
      <child xsi:type="cvl:Choice" name="Internet"/>
    </child>
    <child xsi:type="cvl:Choice" name="PresenceSimulator" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="LightSimulation"/>
      <child xsi:type="cvl:Choice" name="BlindSimulation"/>
      <child xsi:type="cvl:Choice" name="MusicSimulation"/>
    </child>
    <child xsi:type="cvl:Choice" name="FireControl" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="FirstAidGroup" isImpliedByParent="true">
        <child xsi:type="cvl:Choice" name="FireDepartment"/>
        <child xsi:type="cvl:Choice" name="OtherGroup"/>
      </child>
      <child xsi:type="cvl:Choice" name="FireAlarm" isImpliedByParent="true">
        <child xsi:type="cvl:Choice" name="Siren"/>
        <child xsi:type="cvl:Choice" name="Bell"/>
        <child xsi:type="cvl:Choice" name="Light"/>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="DoorLock" isImpliedByParent="true">
      <child xsi:type="cvl:Choice" name="KeypadReader"/>
      <child xsi:type="cvl:Choice" name="CardReader"/>
      <child xsi:type="cvl:Choice" name="FingerprintReader"/>
    </child>
  </ownedVSpec>
  <ownedConstraint xsi:type="cvl:OpaqueConstraint" constraint="SmartHome"/>
</cvl:ConfigurableUnit>

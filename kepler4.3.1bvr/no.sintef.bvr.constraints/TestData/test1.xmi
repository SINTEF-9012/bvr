<bvr:BVRModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:bvr="http:///bvr.ecore/2.0.0">
  <packageElement
      xsi:type="bvr:PrimitveType"
      name="int"
      type="Integer"/>
  <variabilityModel
      xsi:type="bvr:Choice"
      name="office">
    <member
        xsi:type="bvr:Choice"
        name="copier"/>
    <member
        xsi:type="bvr:VClassifier"
        name="PrinterPool">
      <member
          xsi:type="bvr:Choice"
          name="scan"/>
      <member
          xsi:type="bvr:Choice"
          name="printer">
        <variable
            name="Speed"
            type="//@packageElement.0"/>
        <variable
            name="minSpeed"
            type="//@packageElement.0"/>
        <member
            xsi:type="bvr:Choice"
            name="HighSpeed"/>
        <member
            xsi:type="bvr:Choice"
            name="EmergencyPower">
          <variable
              name="threshold"
              type="//@packageElement.0"/>
        </member>
      </member>
      <member
          xsi:type="bvr:Choice"
          name="fax"/>
    </member>
  </variabilityModel>
</bvr:BVRModel>

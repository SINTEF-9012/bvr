<?xml version="1.0" encoding="ASCII"?>
<bvr:ConfigurableUnit xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bvr="http:///bvr.ecore" name="APL">
  <ownedVSpec xsi:type="bvr:Choice" name="APL" isImpliedByParent="true">
    <groupMultiplicity upper="1" lower="1"/>
    <child xsi:type="bvr:Choice" name="mainAPL">
      <child xsi:type="bvr:Choice" name="Java" isImpliedByParent="true">
        <groupMultiplicity upper="1" lower="1"/>
        <child xsi:type="bvr:Choice" name="mainJava">
          <child xsi:type="bvr:Choice" name="reform"/>
          <child xsi:type="bvr:Choice" name="jedi"/>
          <child xsi:type="bvr:Choice" name="jak2java" isImpliedByParent="true"/>
          <child xsi:type="bvr:Choice" name="bali" isImpliedByParent="true"/>
        </child>
      </child>
      <child xsi:type="bvr:Choice" name="Xml">
        <groupMultiplicity upper="1" lower="1"/>
        <child xsi:type="bvr:Choice" name="xc"/>
        <child xsi:type="bvr:Choice" name="xak"/>
      </child>
      <child xsi:type="bvr:Choice" name="cpp"/>
      <child xsi:type="bvr:Choice" name="Gui">
        <groupMultiplicity upper="1" lower="1"/>
        <child xsi:type="bvr:Choice" name="mainGUI">
          <child xsi:type="bvr:Choice" name="me"/>
          <child xsi:type="bvr:Choice" name="mmatrix"/>
          <child xsi:type="bvr:Choice" name="guidsl"/>
          <child xsi:type="bvr:Choice" name="web"/>
        </child>
      </child>
      <child xsi:type="bvr:Choice" name="drc"/>
      <child xsi:type="bvr:Choice" name="bc">
        <child xsi:type="bvr:Choice" name="bcjak2java"/>
      </child>
      <child xsi:type="bvr:Choice" name="aj">
        <child xsi:type="bvr:Choice" name="jak2aj"/>
      </child>
    </child>
  </ownedVSpec>
  <ownedConstraint xsi:type="bvr:OpaqueConstraint" constraint="bc implies mmatrix"/>
  <ownedConstraint xsi:type="bvr:OpaqueConstraint" constraint="me implies mmatrix"/>
</bvr:ConfigurableUnit>

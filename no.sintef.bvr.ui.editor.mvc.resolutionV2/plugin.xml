<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="no.sintef.bvr.ui.editor.mvc.resolutionV2.MVCResolutionEditorV2"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="bvr"
            icon="icons/sample.gif"
            id="no.sintef.bvr.ui.editor.eclipse.editors.BVRResolutionV2EditorMVC"
            name="BVR Resolution Editor(MVC)">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Undo Command"
            id="no.sintef.bvr.ui.editor.mvc.resolutionV2.command.undoCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="no.sintef.bvr.ui.editor.mvc.resolutionV2.command.undoCommand"
            class="no.sintef.bvr.ui.editor.mvc.resolutionV2.handler.Undo">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="no.sintef.bvr.ui.editor.mvc.resolutionV2.command.undoCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+Z"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="no.sintef.bvr.ui.editor.mvc.resolutionV2.toolbars.undoToolbar">
            <command
                  commandId="no.sintef.bvr.ui.editor.mvc.resolutionV2.command.undoCommand"
                  icon="icons/undo.png"
                  tooltip="Undo"
                  id="no.sintef.bvr.ui.editor.mvc.resolutionV2.undoCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   
   
   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Undo Command"
            id="no.sintef.bvr.ui.editor.mvc.resolutionV2.command.redoCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="no.sintef.bvr.ui.editor.mvc.resolutionV2.command.redoCommand"
            class="no.sintef.bvr.ui.editor.mvc.resolutionV2.handler.Redo">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="no.sintef.bvr.ui.editor.mvc.resolutionV2.command.redoCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+Y"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="no.sintef.bvr.ui.editor.mvc.resolutionV2.toolbars.redoToolbar">
            <command
                  commandId="no.sintef.bvr.ui.editor.mvc.resolutionV2.command.redoCommand"
                  icon="icons/redo.png"
                  tooltip="Redo"
                  id="no.sintef.bvr.ui.editor.mvc.resolutionV2.redoCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
</plugin>

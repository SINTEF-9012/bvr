<?xml version="1.0" encoding="ASCII"?>
<cvl:ConfigurableUnit xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cvl="http:///cvl.ecore">
  <ownedVSpec xsi:type="cvl:Choice" name="ArcadeGame" xsi:schemaLocation="http:///cvl.ecore CVLMetamodel.ecore">
    <child xsi:type="cvl:Choice" name="UseCases">
      <child xsi:type="cvl:Choice" name="CheckPreviousBestScore"/>
      <child xsi:type="cvl:Choice" name="SaveScore"/>
      <child xsi:type="cvl:Choice" name="SaveGame"/>
      <child xsi:type="cvl:Choice" name="ExitGame"/>
      <child xsi:type="cvl:Choice" name="InstallGame"/>
      <child xsi:type="cvl:Choice" name="UninstallGame"/>
      <child xsi:type="cvl:Choice" name="ListGame"/>
      <child xsi:type="cvl:Choice" name="PlayGame">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="PlayBrickles"/>
        <child xsi:type="cvl:Choice" name="PlayPong"/>
        <child xsi:type="cvl:Choice" name="PlayBowling"/>
        <child xsi:type="cvl:Choice" name="Initialization">
          <child xsi:type="cvl:Choice" name="AnimationLoop"/>
        </child>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="ClassDiagram">
      <child xsi:type="cvl:Choice" name="GameSprite">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="SpritePair"/>
        <child xsi:type="cvl:Choice" name="Rectangle">
          <child xsi:type="cvl:Choice" name="Size"/>
          <child xsi:type="cvl:Choice" name="Point"/>
        </child>
        <child xsi:type="cvl:Choice" name="MovableSprite">
          <groupMultiplicity upper="-1" lower="1"/>
          <child xsi:type="cvl:Choice" name="Velocity"/>
          <child xsi:type="cvl:Choice" name="Paddle">
            <groupMultiplicity upper="-1" lower="1"/>
            <child xsi:type="cvl:Choice" name="TopPaddle"/>
            <child xsi:type="cvl:Choice" name="BottomPaddle"/>
          </child>
          <child xsi:type="cvl:Choice" name="Puck"/>
          <child xsi:type="cvl:Choice" name="BowlingBall"/>
          <child xsi:type="cvl:Choice" name="BowlingPin"/>
        </child>
        <child xsi:type="cvl:Choice" name="StationarySprite">
          <groupMultiplicity upper="-1" lower="1"/>
          <child xsi:type="cvl:Choice" name="Wall">
            <groupMultiplicity upper="-1" lower="1"/>
            <child xsi:type="cvl:Choice" name="Leftpong"/>
            <child xsi:type="cvl:Choice" name="Rightpont"/>
            <child xsi:type="cvl:Choice" name="Leftbrickles"/>
            <child xsi:type="cvl:Choice" name="Rightbrickles"/>
          </child>
          <child xsi:type="cvl:Choice" name="Brick"/>
          <child xsi:type="cvl:Choice" name="BrickPile"/>
          <child xsi:type="cvl:Choice" name="Ceilingbrickles"/>
          <child xsi:type="cvl:Choice" name="Floorbrickles"/>
          <child xsi:type="cvl:Choice" name="Lane"/>
          <child xsi:type="cvl:Choice" name="Gutter"/>
          <child xsi:type="cvl:Choice" name="Edge"/>
          <child xsi:type="cvl:Choice" name="EndofAlley"/>
          <child xsi:type="cvl:Choice" name="RackofPins"/>
          <child xsi:type="cvl:Choice" name="ScoreBoard"/>
          <child xsi:type="cvl:Choice" name="Floorpong"/>
          <child xsi:type="cvl:Choice" name="Ceilingpong"/>
          <child xsi:type="cvl:Choice" name="DividingLine"/>
        </child>
      </child>
      <child xsi:type="cvl:Choice" name="Pucksupply"/>
      <child xsi:type="cvl:Choice" name="Board">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="PongBoard"/>
        <child xsi:type="cvl:Choice" name="BricklesBoard"/>
        <child xsi:type="cvl:Choice" name="BowlingBoard"/>
      </child>
      <child xsi:type="cvl:Choice" name="Menu">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="Pong"/>
        <child xsi:type="cvl:Choice" name="Brickles"/>
        <child xsi:type="cvl:Choice" name="Bowling"/>
      </child>
      <child xsi:type="cvl:Choice" name="GameMenu">
        <groupMultiplicity upper="-1" lower="1"/>
        <child xsi:type="cvl:Choice" name="PongGameMenu"/>
        <child xsi:type="cvl:Choice" name="BricklesGameMenu"/>
        <child xsi:type="cvl:Choice" name="BowlingGameMenu"/>
      </child>
    </child>
  </ownedVSpec>
</cvl:ConfigurableUnit>

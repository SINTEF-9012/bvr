    <child xsi:type="cvl:Choice" name="FireControl">
      <child xsi:type="cvl:Choice" name="FirstAidGroup">
        <child xsi:type="cvl:Choice" name="FireDepartment"/>
        <child xsi:type="cvl:Choice" name="OtherGroup"/>
      </child>
      <child xsi:type="cvl:Choice" name="FireAlarm">
        <child xsi:type="cvl:Choice" name="Siren"/>
        <child xsi:type="cvl:Choice" name="Bell"/>
        <child xsi:type="cvl:Choice" name="Light"/>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="DoorLock">
      <child xsi:type="cvl:Choice" name="KeypadReader"/>
      <child xsi:type="cvl:Choice" name="CardReader"/>
      <child xsi:type="cvl:Choice" name="FingerprintReader"/>
    </child>

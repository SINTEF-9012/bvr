<?xml version="1.0" encoding="ASCII"?>
<cvl:ConfigurableUnit xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cvl="http:///cvl.ecore" xsi:schemaLocation="http:///cvl.ecore CVLMetamodel.ecore">
  <ownedVSpec xsi:type="cvl:Choice" name="SmartHome">
    <child xsi:type="cvl:Choice" name="EnvironmentControl">
      <child xsi:type="cvl:Choice" name="MeasurementUnits">
        <child xsi:type="cvl:Choice" name="Celsius"/>
        <child xsi:type="cvl:Choice" name="Fahrenheit"/>
      </child>
      <child xsi:type="cvl:Choice" name="TemperatureManagement">
        <child xsi:type="cvl:Choice" name="HeatingManagement"/>
        <child xsi:type="cvl:Choice" name="SmartHeatingManagement"/>
      </child>
      <child xsi:type="cvl:Choice" name="WindowsManagement">
        <child xsi:type="cvl:Choice" name="ManualWindows"/>
        <child xsi:type="cvl:Choice" name="AutomaticWindows"/>
      </child>
    </child>
    <child xsi:type="cvl:Choice" name="LightManagement">
      <child xsi:type="cvl:Choice" name="SmartLightManagement"/>
      <child xsi:type="cvl:Choice" name="Pre_definedValues"/>
      <child xsi:type="cvl:Choice" name="SimpleControl"/>
    </child>
    <child xsi:type="cvl:Choice" name="GUI">
      <child xsi:type="cvl:Choice" name="TouchScreen"/>
      <child xsi:type="cvl:Choice" name="Mobile"/>
      <child xsi:type="cvl:Choice" name="Internet"/>
    </child>
    <child xsi:type="cvl:Choice" name="PresenceSimulator">
      <child xsi:type="cvl:Choice" name="LightSimulation"/>
      <child xsi:type="cvl:Choice" name="BlindSimulation"/>
      <child xsi:type="cvl:Choice" name="MusicSimulation"/>

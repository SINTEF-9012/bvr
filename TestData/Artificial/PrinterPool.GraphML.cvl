<?xml version="1.0" encoding="ASCII"?>
<cvl:ConfigurableUnit
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:cvl="http:///cvl.ecore"
    xsi:schemaLocation="http:///cvl.ecore CVLMetamodel.ecore">
  <ownedVSpec
      xsi:type="cvl:VClassifier"
      name="PrinterPool">
    <child xsi:type="cvl:VClassifier"
        name="Printer">
      <groupMultiplicity
          upper="1"
          lower="1"/>
      <child
          xsi:type="cvl:Choice"
          name="x600dpi"/>
      <child
          xsi:type="cvl:Choice"
          name="x1000dpi"/>
      <child
          xsi:type="cvl:Choice"
          name="x300dpi"/>
      <child
          xsi:type="cvl:Variable"
          name="resolution:int"/>
      <instanceMultiplicity
          upper="-1"
          lower="1"/>
    </child>
    <child xsi:type="cvl:VClassifier"
        name="Scanner">
      <groupMultiplicity
          upper="1"
          lower="1"/>
      <child
          xsi:type="cvl:Choice"
          name="x300dpi"/>
      <child
          xsi:type="cvl:Choice"
          name="x1000dpi"/>
      <child
          xsi:type="cvl:Variable"
          name="resolution:int"/>
      <instanceMultiplicity
          upper="-1"/>
    </child>
    <instanceMultiplicity
        upper="-1"/>
  </ownedVSpec>
  <ownedConstraint
      xsi:type="cvl:OpaqueConstraint"
      context="//@ownedVSpec.0"
      constraint="Printer->max(resolution) &lt;= &#xA;Scanner->max(resolution)"/>
  <ownedConstraint
      xsi:type="cvl:OpaqueConstraint"
      context="//@ownedVSpec.0/@child.0/@child.0"
      constraint="resolution = 300"/>
  <ownedConstraint
      xsi:type="cvl:OpaqueConstraint"
      context="//@ownedVSpec.0/@child.0/@child.1"
      constraint="resolution = 300"/>
  <ownedConstraint
      xsi:type="cvl:OpaqueConstraint"
      context="//@ownedVSpec.0/@child.0/@child.2"
      constraint="resolution = 300"/>
  <ownedConstraint
      xsi:type="cvl:OpaqueConstraint"
      context="//@ownedVSpec.0/@child.1/@child.0"
      constraint="resolution = 300"/>
  <ownedConstraint
      xsi:type="cvl:OpaqueConstraint"
      context="//@ownedVSpec.0/@child.1/@child.1"
      constraint="resolution = 1000"/>
</cvl:ConfigurableUnit>
